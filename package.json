  "author": "Seistreinta",
  "description": "Backend framework for Tepache",
  "version": "1.0.0",
  "dependencies": {
    "@axelspringer/graphql-google-pubsub": "^2.1.0",
    "@google-cloud/error-reporting": "^3.0.5",
    "@google-cloud/logging-winston": "^5.3.0",
    "@google-cloud/storage": "^6.9.5",
    "@google-cloud/trace-agent": "^7.1.2",
    "@prisma/client": "^4.12.0",
    "@types/lodash": "^4.14.192",
    "@types/uuid": "9.0.1",
    "apollo-server-errors": "3.3.1",
    "apollo-server-express": "^3.10.2",
    "apollo-server-plugin-cloud-trace": "0.0.3",
    "auth0": "^3.3.0",
    "body-parser": "^1.20.2",
    "cors": "2.8.5",
    "cross-fetch": "3.1.5",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "lodash": "4.17.21",
    "make-promises-safe": "5.1.0",
    "nexus": "^1.3.0",
    "uuid": "^9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/object-hash": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "2.8.7",
    "prisma": "4.12.0",
    "semantic-release": "^21.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  },
  "scripts": {
    "format": "npx prettier --write './**/*.{ts,md}'",
    "test": "npm -s run test:unit && npm -s run test:acceptance",
    "dev": "ts-node-dev --transpile-only src/app.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "start": "node dist/app",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "introspect": "prisma db pull",
    "sync": "prisma db push",
    "studio": "prisma studio",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "nexus:reflection": "ts-node src/app",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "codequality": "eslint . --debug",
    "test:unit": "jest tests/unit",
    "test:acceptance": "jest tests/acceptance"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
